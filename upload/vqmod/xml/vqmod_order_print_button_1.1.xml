<modification>        <id>Order Print Button</id>        <version>1.1</version>        <vqmver>2.2.1</vqmver>        <author>@Brunte27</author>		<file name="catalog/view/theme/*/template/account/order_info.tpl">		<operation error="log">			<search position="after">				<![CDATA[<?php echo $heading_title; ?></h1>]]>			</search>			<add>				<![CDATA[<script type="text/javascript">function printDiv(skrivut){var dd=document.getElementById(skrivut).innerHTML,ee=document.body.innerHTML;document.body.innerHTML=dd,window.print(),document.body.innerHTML=ee}</script>]]>			</add>		</operation>		<operation error="log">			<search position="before">				<![CDATA[<h1]]>			</search>			<add>				<![CDATA[<div id="skrivut">]]>			</add>		</operation>		<operation error="log">			<search position="before">				<![CDATA[<div class="buttons">]]>			</search>			<add>				<![CDATA[</div>]]>			</add>		</operation>		<operation error="log">			<search position="replace">				<![CDATA[<div class="right"><a href="<?php echo $continue; ?>" class="button"><?php echo $button_continue; ?></a></div>]]>			</search>			<!-- EDIT THIS LINE BELOW	change red to the color you want-->			<add>				<![CDATA[<div class="right"><a href="javascript:printDiv('skrivut');" class="button" /><span style="color: grey;"><?php echo $button_skrivut; ?></a> <a href="<?php echo $continue; ?>" class="button"><span style="color: grey;"><?php echo $button_continue; ?> </span></a></div>]]>			</add>		</operation>        </file>		<file name="catalog/language/english/english.php">		<operation>			<search position="after"><![CDATA[// Buttons]]></search>			<add><![CDATA[$_['button_skrivut']        = 'Print';]]></add>		</operation>		</file><!-- EDIT THIS LINE BELOW	your language change * to the language you use for example to swedish do it on the line with * and change swedish.php to your language file name--><!--=============================**************>===and change where it says ****** to what you want it to say in your language.-->	<file name="catalog/language/lithuanian/lithuanian.php">		<operation>			<search position="after"><![CDATA[// Buttons]]></search>			<add><![CDATA[$_['button_skrivut']        = 'Spausdinti';]]></add>		</operation>	</file>	<file name="catalog/controller/account/order.php">		<operation>			<search position="after" error="log"><![CDATA[$this->data['button_continue'] = $this->language->get('button_continue');]]></search>			<add><![CDATA[$this->data['button_skrivut'] = $this->language->get('button_skrivut');]]></add>		</operation>	</file></modification>